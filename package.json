{
  "name": "unity-wallet",
  "description": "Unity Wallet (Creat React Native + React Native Web + Electron)",
  "version": "0.0.1",
  "private": true,
  "homepage": "./src/",
  "main": "public/electron.js",
  "author": {
    "name": "Unity Dev",
    "email": "your.email@domain.com",
    "url": "https://unity.sg"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "some.id.ofyours",
    "productName": "Unity Wallet",
    "copyright": "Copyright Â© 2019 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "public/icon.png",
      "target": "nsis",
      "legalTrademarks": "Unity Wallet",
      "publisherName": "unity",
      "certificateFile": "private/unity.pfx",
      "verifyUpdateCodeSignature": false
    },
    "mac": {
      "category": "public.app-category.business",
      "icon": "public/icon.png",
      "target": "dmg",
      "identity": "private/unity.pfx"
    },
    "linux": {
      "category": "Utility",
      "icon": "public/icon.png",
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.0",
    "@react-navigation/core": "^3.5.1",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "electron-is-dev": "^1.1.0",
    "react": "16.9.0",
    "react-art": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-responsive-ui": "^2.1.1",
    "react-native-router-flux": "^4.0.6",
    "react-native-screens": "^2.0.0-alpha.3",
    "react-native-storage": "^1.0.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-app-rewired": "^2.1.4",
    "react-test-renderer": "16.9.0",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "web": "EXTEND_ESLINT=true react-app-rewired start",
    "web-build": "EXTEND_ESLINT=true react-app-rewired build",
    "desktop": "concurrently \"cross-env BROWSER=none yarn web\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop-build": "concurrently \"rm -rf build dist && yarn web-build\" \"wait-on build/index.html && npx electron-builder -mwl --publish never\"",
    "desktop-build-mac": "concurrently \"rm -rf build dist && yarn web-build\" \"wait-on build/index.html && npx electron-builder -m --publish never\"",
    "desktop-build-win": "concurrently \"rm -rf build dist && yarn web-build\" \"wait-on build/index.html && npx electron-builder -w --publish never\"",
    "desktop-build-linux": "concurrently \"rm -rf build dist && yarn web-build\" \"wait-on build/index.html && npx electron-builder -l --publish never\"",
    "desktop-ship": "concurrently \"yarn web-build\" \"wait-on build/index.html npx electron-builder -mwl -p always\"",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "make-cert-win": "npx electron-builder create-self-signed-cert -p unity",
    "postinstall": "jetifier -r",
    "clear": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf build/ & rm -rf node_modules/; yarn cache clean --force; yarn install"
  }
}
